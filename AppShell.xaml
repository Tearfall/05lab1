<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp12.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp12.Pages"
    Shell.FlyoutBehavior="Flyout">


    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent
            Title="Fallen"
            ContentTemplate="{DataTemplate local:Fallen}"
            Icon="fallen.jpeg" />

        <ShellContent
            Title="Raining In Manila"
            ContentTemplate="{DataTemplate local:RainingInManila}"
            Icon="raining.png" />

        <ShellContent
            Title="White Toyota"
            ContentTemplate="{DataTemplate local:WhiteToyota}"
            Icon="whitetoyota.jpeg" />
    </FlyoutItem>
    
    
    

</Shell>
